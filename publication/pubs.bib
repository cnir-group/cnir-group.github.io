@inproceedings{aydogan_texture_2008,
	title = {Texture based classification and  segmentation of tissues using {DT}-{CWT} feature extraction methods},
	doi = {10.1109/CBMS.2008.46},
	abstract = {In this study, four different dual-tree complex wavelet (DT-CWT) based texture feature extraction methods are developed and compared to segment and classify tissues. Methods that are proposed in this study are based on local energy calculations of sub-bands. Two of the methods use rotation variant texture features and the other two use rotation invariant features. The methods are tested on two texture compositions from the Brodatz texture database and two actual magnetic resonance (MR) images. Results show that there is not a significant difference between using rotation variant or invariant features. On the other hand, for the same Brodatz textures, all DT-CWT based feature extraction methods are competitive with other filtering approaches.},
	booktitle = {2008 21st {IEEE} {International} {Symposium} on {Computer}-{Based} {Medical} {Systems}},
	author = {Aydogan, Dogu Baran and Hannula, Markus and Arola, Tuukka and Dastidar, Prasun and Hyttinen, Jari},
	month = jun,
	year = {2008},
	note = {ISSN: 1063-7125},
	keywords = {Biomedical computing, Biomedical imaging, Computed tomography, Discrete wavelet transforms, Feature extraction, Image databases, Image segmentation, Magnetic resonance, Spatial databases, Testing},
	pages = {614--619},
	file = {IEEE Xplore Abstract Record:/home/baran/Zotero/storage/6L2UCY3K/4562069.html:text/html},
}

@inproceedings{aydogan_comparison_2008,
	address = {Riga},
	series = {{IFMBE} {Proceedings}},
	title = {Comparison of {DT}-{CWT} {Based} {Rotation} {Variant} and {Invariant} {Methods} on {Tissue} {Characterization}},
	isbn = {978-3-540-69367-3},
	doi = {10.1007/978-3-540-69367-3_127},
	abstract = {The goal of this study is to compare the effect of rotation variance on dual-tree complex wavelet transform (DT-CWT) based tissue characterization methods. Rotation variant features are especially useful when estimating orientations of textures. On the other hand, since tissues can turn and orient themselves on different directions, in segmentation or classification problems, rotation invariance becomes important. The methods are tested on two texture compositions from the Brodatz texture database and two actual magnetic resonance (MR) images. Results show that, the classification performance is not significantly affected by using rotation variant or invariant methods. On the other hand, regarding segmentation, rotation invariant DT-CWT features performed slightly better compared to rotation variant features.},
	language = {en},
	booktitle = {14th {Nordic}-{Baltic} {Conference} on {Biomedical} {Engineering} and {Medical} {Physics}},
	publisher = {Springer},
	author = {Aydogan, Baran and Hannula, M. and Arola, T. and Dastidar, P. and Hyttinen, J.},
	editor = {Katashev, Alexei and Dekhtyar, Yuri and Spigulis, Janis},
	year = {2008},
	keywords = {DTCWT, texture analysis, tissue characterization},
	pages = {473--476},
}

