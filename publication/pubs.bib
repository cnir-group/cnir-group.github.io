	keywords = {Bayesian inference, Probabilistic tractography, Visual pathway},
	pages = {201--209},
	file = {Full Text PDF:/home/baran/Zotero/storage/BA5GEUI9/Aydogan and Shi - 2016 - Probabilistic Tractography for Topographically Org.pdf:application/pdf},
